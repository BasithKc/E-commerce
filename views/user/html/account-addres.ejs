    <script>
    //Add address button
     const addAddressBtn = document.querySelector('.add-address-btn')

     //Add address form div
    const addAddressForm = document.querySelector('.add-address-form')

    //cancel button of add address form
    const cancelBtn = document.querySelector('.cancel-btn')

    //cancel button of edit address form
    const cancelBtnEdit = document.querySelectorAll('.edit-cancel-btn')

    const moreOptionBtns = document.querySelectorAll('.more-option-click')//more option button
    const moreOption = document.querySelectorAll('.more-option') //more options div

    const editAddressBtn  = document.querySelectorAll('.address-edit') //eidt address button 
    const editAddressForm = document.querySelectorAll('.edit-address-form')//edit address form

    //when click add address btn
    addAddressBtn.addEventListener('click', (event) => {
        addAddressForm.style.display = 'block'
    })

    //when click on the address add form's cancel button
    cancelBtn.addEventListener('click', (event) => {
        event.preventDefault()
        
        // Hide the form and show the button
        addAddressForm.style.display='none'
        addAddressBtn.style.display='block'
    })
     

    //when the mouse enters the more option button in each addresss
    moreOptionBtns.forEach(button => {
        
        button.addEventListener('mouseenter', (event) => {
            const index = event.target.getAttribute( 'data-index' )
            moreOption[index].style.display='block'
        })
    })

    //when the mouse leaves the more option button in each addresss
    moreOption.forEach(button => {
        button.addEventListener('mouseleave', () => {
            const index = event.target.getAttribute( 'data-index' )
            moreOption[index].style.display='none'
        })

    })
    
    editAddressBtn.forEach((button, index)=>{
        button.addEventListener("click", function(event){
            event.preventDefault()
            //Hide all other forms and show only one that is clicked
          
            const index = this.getAttribute( 'data-index' )

           const formOfEdit =  editAddressForm[index]

           formOfEdit.style.display = 'block'
        
        // Select the previous div related to this cancel button
        const previousDiv = formOfEdit.previousElementSibling;
        // Display the previous div
        previousDiv.style.display = 'none';

          
        })
    })
    
    cancelBtnEdit.forEach(button => {
        button.addEventListener('click',function (event)  {
        event.preventDefault()

        const index = this.getAttribute( 'data-index' )
        console.log(index)

        // Select the edit address form related to this cancel button
        var formOfEdit = editAddressForm[index]

        // Display the form
        formOfEdit.style.display = 'none';

        // Select the previous div related to this cancel button
        const previousDiv = formOfEdit.previousElementSibling;
        // Display the previous div
        previousDiv.style.display = 'block';

        })
    })
    </script>
